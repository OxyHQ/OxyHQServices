{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@oxyhq/services": [
        "../services/src"
      ],
      "@oxyhq/services/*": [
        "../services/src/*"
      ],
      "@oxyhq/services/core": [
        "../services/src/core"
      ],
      "@oxyhq/services/full": [
        "../services/src"
      ],
      "@oxyhq/services/ui": [
        "../services/src/ui"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "../services/src/**/*.ts",
    "../services/src/**/*.tsx"
  ],
  "extends": "expo/tsconfig.base"
}