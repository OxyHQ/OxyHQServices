{
  "name": "@oxyhq/sdk",
  "version": "1.0.0",
  "description": "OxyHQ SDK Monorepo â€” core, auth, services, and API packages",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/auth-sdk",
    "packages/services",
    "packages/api",
    "packages/auth",
    "packages/accounts",
    "packages/test-app"
  ],
  "scripts": {
    "build:all": "npm run core:build && npm run auth:build && npm run services:build && npm run build --workspaces --if-present",
    "build:workspaces": "npm run build --workspaces --if-present",
    "start": "cd packages/api && npm start",
    "dev": "npm run dev --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "install:all": "npm install",
    "core:build": "npm run build -w @oxyhq/core",
    "auth:build": "npm run build -w @oxyhq/auth",
    "services:build": "npm run build -w @oxyhq/services",
    "services:dev": "npm run dev -w @oxyhq/services --if-present",
    "services:test": "npm run test -w @oxyhq/services",
    "api:build": "npm run build -w @oxyhq/api",
    "api:dev": "npm run dev -w @oxyhq/api",
    "api:start": "npm run start -w @oxyhq/api",
    "accounts:build": "npm run build -w accounts",
    "accounts:dev": "npm run start -w accounts",
    "accounts:start": "npm run start -w accounts"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.6"
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxyhq/sdk.git"
  },
  "keywords": [
    "monorepo",
    "oxyhq",
    "sdk",
    "authentication",
    "api",
    "services"
  ],
  "author": "OxyHQ",
  "license": "MIT",
  "overrides": {
    "send": "0.19.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "dependencies": {
    "expo-contacts": "~15.0.11",
    "is-arrayish": "^0.3.2"
  }
}
